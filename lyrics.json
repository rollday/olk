  "lyrics": [
      {"time": "00:00.00", "text": "One Last Kiss - 宇多田光 (宇多田ヒカル)"},
      {"time": "00:09.57", "text": "词：宇多田ヒカル"},
      {"time": "00:14.67", "text": "曲：宇多田ヒカル"},
      {"time": "00:20.69", "text": "初めてのルーブルは", "translation": "第一次去卢浮宫时"},
      {"time": "00:23.01", "text": "なんてことはなかったわ", "translation": "并没有什么特别的感觉"},
      {"time": "00:25.10", "text": "私だけのモナリザ", "translation": "因为独属于我的蒙娜丽莎"},
      {"time": "00:26.94", "text": "もうとっくに出会ってたから", "translation": "我早已遇见"},
      {"time": "00:29.37", "text": "初めてあなたを見た", "translation": "初次遇见你的那天"},
      {"time": "00:31.16", "text": "あの日動き出した歯車", "translation": "齿轮开始转动"},
      {"time": "00:33.67", "text": "止められない喪失の予感", "translation": "无法停止那将要失去什么的预感"},
      {"time": "00:38.02", "text": "もういっぱいあるけど", "translation": "虽说已经有很多次了"},
      {"time": "00:43.79", "text": "もう一つ増やしましょう", "translation": "让我们再来一个吻吧"},
      {"time": "00:47.75", "text": "(Can you give me one last kiss?)"},
      {"time": "00:52.39", "text": "忘れたくないこと", "translation": "不想遗忘之事"},
      {"time": "00:55.22", "text": "Oh oh oh oh oh"},
      {"time": "01:01.07", "text": "忘れたくないこと", "translation": "不愿遗忘之事"},
      {"time": "01:03.79", "text": "Oh oh oh oh oh"},
      {"time": "01:09.63", "text": "I love you more than you'll ever know"},
      {"time": "01:20.84", "text": "「写真が苦手なんだ」", "translation": "「我不擅长拍照」"},
      {"time": "01:22.78", "text": "でもそんなものはいらないわ", "translation": "但那种事情其实无关紧要"},
      {"time": "01:24.92", "text": "あなたが焼きついたまま", "translation": "你的身影已经烙在"},
      {"time": "01:26.90", "text": "私の心のプロジェクター", "translation": "我心中的放映机里"},
      {"time": "01:29.26", "text": "寂しくないふりしてた", "translation": "都假装着自己并不孤独"},
      {"time": "01:31.35", "text": "まあ そんなのお互い様か", "translation": "嘛，我们就是那样"},
      {"time": "01:33.57", "text": "誰かを求めることは", "translation": "如果谁去找了对方"},
      {"time": "01:35.57", "text": "即ち傷つくことだった", "translation": "就会徒增伤痕"},
      {"time": "01:38.19", "text": "Oh can you give me one last kiss?", "translation": "Oh, can you give me one last kiss？"},
      {"time": "01:43.72", "text": "燃えるようなキスをしよう", "translation": "来一个火热的吻吧"},
      {"time": "01:48.05", "text": "忘れたくても", "translation": "火热到即使想忘"},
      {"time": "01:52.44", "text": "忘れられないほど", "translation": "也难以忘怀"},
      {"time": "01:55.19", "text": "Oh oh oh oh oh"},
      {"time": "02:01.00", "text": "I love you more than you'll ever know"},
      {"time": "02:03.76", "text": "Oh oh oh oh oh"},
      {"time": "02:09.61", "text": "I love you more than you'll ever know"},
      {"time": "02:12.51", "text": ""},
      {"time": "02:29.52", "text": "もう分かっているよ", "translation": "我已经明白了"},
      {"time": "02:35.05", "text": "この世の終わりでも", "translation": "就算这个世界走向终结"},
      {"time": "02:39.63", "text": "年をとっても", "translation": "就算年龄渐长"},
      {"time": "02:46.14", "text": "忘れられない人", "translation": "你也是我无法忘记的人"},
      {"time": "02:48.71", "text": "Oh oh oh oh oh"},
